{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "<my-region>"
    },
    "postgreSqlServerName": {
      "value": "<my-data-landing-zone-name>-postgresqlserver001"
    },
    "postgreSqlDatabaseName": {
      "value": "DataDomain001"
    },
    "postgreSqlSkuName": {
      "value": "GP_Gen5_2"
    },
    "postgreSqlSkuTier": {
      "value": "GeneralPurpose"
    },
    "postgreSqlSkuCapacity": {
      "value": 2
    },
    "postgreSqlSkuSize": {
      "value": 5120
    },
    "postgreSqlSkuFamily": {
      "value": "Gen5"
    },
    "postgreSqlServerAdministratorLogin": {
      "value": "ServerMainUser"
    },
    "postgreSqlServerAdministratorLoginPassword": {
      "value": "<your-secure-password>"
    },
    "postgreSqlAdminGroupName": {
      "value": ""
    },
    "postgreSqlAdminGroupObjectID": {
      "value": ""
    },
    "keyVaultId": {
      "value": "/subscriptions/<my-data-landing-zone-subscription-id>/resourceGroups/<my-data-landing-zone-name>-rg/providers/Microsoft.KeyVault/vaults/<my-data-landing-zone-name>-keyvault001"
    },
    "subnetId": {
      "value": "<my-subnet-resource-id>"
    },
    "privateDnsZoneId": {
      "value": "/subscriptions/<my-management-subscription-id>/resourceGroups/<my-global-dns-resource-group-name>/providers/Microsoft.Network/privateDnsZones/privatelink.postgres.database.azure.com"
    }
  }
}

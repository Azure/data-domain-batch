{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "location": {
      "value": "{regionName}"
    },
    "postgreSqlServerName": {
      "value": "datalanding-postgresqlserver001"
    },
    "postgreSqlDatabaseName": {
      "value": "DataDomain001"
    },
    "postgreSqlSkuName": {
      "value": "GP_Gen5_2"
    },
    "postgreSqlSkuTier": {
      "value": "GeneralPurpose"
    },
    "postgreSqlSkuCapacity": {
      "value": 2
    },
    "postgreSqlSkuSize": {
      "value": 5120
    },
    "postgreSqlSkuFamily": {
      "value": "Gen5"
    },
    "postgreSqlServerAdministratorLogin": {
      "value": "ServerMainUser"
    },
    "postgreSqlServerAdministratorLoginPassword": {
      "value": "<your-secure-password>"
    },
    "postgreSqlAdminGroupName": {
      "value": ""
    },
    "postgreSqlAdminGroupObjectID": {
      "value": ""
    },
    "keyVaultId": {
      "value": "/subscriptions/{dataLandingZoneSubscriptionId}/resourceGroups/datalanding-rg/providers/Microsoft.KeyVault/vaults/datalanding-keyvault001"
    },
    "subnetId": {
      "value": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/virtualNetworks/{vnetName}/subnets/{subnetName}"
    },
    "privateDnsZoneId": {
      "value": "/subscriptions/{subscriptionId}/resourceGroups/{resourceGroupName}/providers/Microsoft.Network/privateDnsZones/privatelink.postgres.database.azure.com"
    }
  }
}
